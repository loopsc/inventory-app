<html>

<head>
    <title>
        <%= title %>
    </title>
</head>

<body>
    <main class="content">
        <%- include('./partials/navbar') %>
            <h1>
                <%= title %>
            </h1>
            <h1>
                <%= book.title %>
            </h1>
            <p>
                <%= book.author %>
            </p>
            <ul>
                <% book.genres.forEach(genre=> { %>
                    <li>
                        <%= genre %>
                    </li>
                    <% }); %>
            </ul>
    </main>
</body>

</html>